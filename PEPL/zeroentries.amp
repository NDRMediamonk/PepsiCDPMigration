<div style="display: none">
  %%[
  /* Ribbon AMPScrit */
   /*Abort email if subscriber has no incomplete activities */
    SET @LoyaltyProgramMember = [LoyaltyProgramMember]
  IF EMPTY (@LoyaltyProgramMember) THEN
   RaiseError("LoyaltyProgramMember Json is empty", true)
  ENDIF
  set @firstName = FirstName
  IF empty(@firstName) OR @firstName == "FIRSTNAME" THEN
    set @ribbon = 'Ready for more activities?'
  ELSE
    set @ribbon = concat(ProperCase(@firstName), ', ready for more activities?')
  ENDIF
  /* variables */
 var @activityCount , @points , @activitiesIds
]%%
 <!-- /*######################################
  ################### Json parsing Start####################
  ######################################*/ -->

<script runat="server">
    Platform.Load("core", "1.1.1");

 // Retrieve the AMPscript variable'
 var emailAddress = Platform.Recipient.GetAttributeValue('emailAddress');
  var jsonString = Platform.Recipient.GetAttributeValue('LoyaltyProgramMember');
    // Step 1: Parse the JSON data
    var data = Platform.Function.ParseJSON(jsonString);

    // Step 2: Extract the Activity_Id values
    var activityIds = [];
    var activities = data[0].Loyalty_Activities_Available; //Points_Balance
    var pointsBalance = data[0].Points_Balance;
     Platform.Variable.SetValue("@points",pointsBalance);
    for (var i = 0; i < activities.length; i++) {
        activityIds.push(activities[i].Activity_Id);
    }
    var matchingRecords = [];
    var deName = "ent.PEPL_Activity_Lookup";
    for (var j = 0; j < activityIds.length; j++) {
        var rows = Platform.Function.LookupRows(deName, "Campaign_Id", activityIds[j]);
        if (rows.length > 0) {
            matchingRecords = matchingRecords.concat(rows);
        }
    }
     var activityLookuptwoRec = [];
     var matchingaAtivityCount = matchingRecords.length;
     Platform.Variable.SetValue("@matchingaAtivityCount", matchingaAtivityCount);
    var activityCount = activityIds.length;
     Platform.Variable.SetValue("@activitiesIds", activityIds);
     Platform.Variable.SetValue("@activityCount", activityCount);
</script>

    <!--######################################
  ################### Json parsing End ####################
  ######################################*/ -->
 %%[
   IF @matchingaAtivityCount < 2 THEN
   RaiseError("This subscriber has no activities to be completed", true)
   ENDIF
  
  /* Active Loyalty  Part start*/
  set @sub_loyalty = [Brand_Affinity]
  set @regex = concat("(", @sub_loyalty, ")")
  IF @sub_loyalty != "PTR" THEN
  set @brand_header_img = Lookup("PTR_Modular_Brands_Attributes", "Header Image URL", "Brand Short Name", @sub_loyalty)
  set @brand_header_link = Lookup("PTR_Modular_Brands_Attributes", "Header Link", "Brand Short Name", @sub_loyalty)
  set @brand_header_alt_text = Lookup("PTR_Modular_Brands_Attributes", "Header Alt Text", "Brand Short Name", @sub_loyalty)
  set @cta_link = @brand_header_link
  ELSE
  set @cta_link = "https://www.tastyrewards.com/en-us/dashboard?__locale__=en#activity"
  ENDIF
  /* Active Loyalty  Part End*/
  ]%%
  %%[
  
  /* Auxiliary variables */
   SET @evergreenID1 = 410114562
   SET @evergreenID2 = 410114575
   SET @criteriaDate = FormatDate(Now(), "s")
    SET @k = 0
    ]%%
  
  %%[ 
  /* Login historical data */
     set @thirdPartyId = AttributeValue("thirdPartyId")
     set @emailAddress = AttributeValue("emailAddress")
     set @sentdate = Now(1)
   ]%%
 
  %%[
  /* Headline AMPScript */
set @FirstName = AttributeValue("FirstName") 
set @firstNameTrimmed = trim(@FirstName)
set @greeting =concat(ProperCase(@firstNameTrimmed))

]%%
    
